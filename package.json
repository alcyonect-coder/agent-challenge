{
  "name": "territory-ai-game",
  "version": "0.1.0",
  "description": "Paper.io-style AI agent game for Nosana Builders Challenge",
  "private": true,
  "scripts": {
    "dev:agent": "mastra dev",
    "dev:ui": "next dev --turbopack",
    "dev:debug": "LOG_LEVEL=debug npm run dev",
    "build:ui": "next build",
    "start:ui": "next start",
    "build:agent": "mastra build",
    "start:agent": "mastra start",
    "lint": "next lint",
    "build": "mastra build && next build",
    "start": "concurrently \"npm:start:agent\" \"npm:start:ui\""
  },
  "dependencies": {
    "@ag-ui/mastra": "0.1.0",
    "@ai-sdk/openai": "^2.0.42",
    "@copilotkit/react-core": "1.10.6",
    "@copilotkit/react-textarea": "1.10.6",
    "@copilotkit/react-ui": "1.10.6",
    "@copilotkit/runtime": "1.10.6",
    "@libsql/client": "^0.15.15",
    "@mastra/client-js": "0.14.1",
    "@mastra/core": "0.19.1",
    "@mastra/libsql": "0.15.0",
    "@mastra/mcp": "^0.13.4",
    "@mastra/memory": "0.15.4",
    "@radix-ui/react-slot": "^1.2.3",
    "@types/chess.js": "^0.13.7",
    "chess.js": "^1.4.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "libsql": "^0.5.22",
    "lucide-react": "^0.545.0",
    "mastra": "0.13.4",
    "next": ">=15.5.4",
    "ollama-ai-provider-v2": "^1.5.0",
    "react": "^19.0.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.1.2",
    "eslint": "^9",
    "eslint-config-next": "15.3.2",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  }
